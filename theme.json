						{
							"fontFamily": "Montserrat",
							"fontWeight": "400,700",
							"fontStyle": "italic",
							"fontStretch": "normal",
							"src": [
								"file:./src/assets/fonts/Montserrat/Montserrat-Italic-VariableFont_wght.ttf"
							]
						}
					]
				}
			],
			"fontSizes": [
				{
					"slug": "base-font",
					"size": "16px",
					"name": "base-font"
				},
				{
					"slug": "design-small",
					"size": "0.813rem",
					"name": "Small"
				},
				{
					"slug": "design-button",
					"size": "1rem",
					"name": "Button"
				},
				{
					"slug": "design-body",
					"size": "1rem",
					"name": "Body"
				},
				{
					"slug": "design-headline",
					"size": "1.25rem",
					"name": "Headline"
				},
				{
					"slug": "design-medium",
					"size": "1.563rem",
					"name": "medium"
				},
				{
					"slug": "design-large",
					"size": "1.938rem",
					"name": "large"
				},
				{
					"slug": "design-xlarge",
					"size": "2.438rem",
					"name": "Xlarge"
				},
				{
					"slug": "design-xxlarge",
					"size": "3.063rem",
					"name": "XXlarge"
				}
			]
		},
		"color": {
			"background": true,
			"custom": true,
			"customDuotone": true,
			"customGradient": true,
			"defaultGradients": false,
			"defaultPalette": false,
			"link": true,
			"text": true,
			"duotone": [],
			"gradients": [],
			"palette": [
				{
					"color": "#FFFFFF",
					"slug": "white",
					"name": "White"
				},
				{
					"color": "#000000",
					"slug": "black",
					"name": "Black"
				},
				{
					"color": "#C5C5C5",
					"slug": "gray-300",
					"name": "Gray 300"
				},
				{
					"color": "#7E7E7E",
					"slug": "gray-600",
					"name": "Gray 600"
				},
				{
					"color": "#141627",
					"slug": "primary-500",
					"name": "Primary 500"
				},
				{
					"color": "#A5A8C0",
					"slug": "secondary-200",
					"name": "Secondary 200"
				},
				{
					"color": "#4A5181",
					"slug": "secondary-400",
					"name": "Secondary 400"
				},
				{
					"color": "#1D2661",
					"slug": "secondary-500",
					"name": "Secondary 500"
				},
				{
					"color": "#11173A",
					"slug": "secondary-700",
					"name": "Secondary 700"
				},
				{
					"color": "#F9D2D4",
					"slug": "tertiary-100",
					"name": "Tertiary 100"
				},
				{
					"color": "#E94D53",
					"slug": "tertiary-400",
					"name": "Tertiary 400"
				},
				{
					"color": "#E32028",
					"slug": "tertiary-500",
					"name": "Tertiary 500"
				},
				{
					"color": "#E32028",
					"slug": "button-1",
					"name": "Button 1"
				},
				{
					"color": "#881318",
					"slug": "button-2",
					"name": "Button 2"
				},
				{
					"color": "#C5C5C5",
					"slug": "button-3",
					"name": "Button 3"
				}
			]
		},
		"layout": {
			"contentSize": "1128px",
			"wideSize": "100vw"
		}
	},
	"styles": {
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--montserrat-sans)",
			"fontSize": "16px"
		},
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--50)"
		},
		"color": {
			"background": "var(--wp--preset--color--secondary-100)",
			"text": "var(--wp--preset--color--black)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--design-xxlarge)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--design-xlarge)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--design-large)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--design-medium)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--design-headline)"
				}
			},
			"button": {
				"color": {
					"background": "var(--wp--preset--color--button-1)",
					"text": "var(--wp--preset--color--white)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--design-button)"
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--button-2)"
					}
				},
				":focus": {
					"color": {
						"background": "var(--wp--preset--color--button-2)"
					}
				},
				":active": {
					"color": {
						"background": "var(--wp--preset--color--button-2)"
					}
				}
			},
			"link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--design-body)"
				},
				"color": {
					"text": "var(--wp--preset--color--button-1)"
				},
				":active": {
					"color": {
						"text": {
							"ref": "styles.color.text"
						}
					}
				}
			}
		},
		"blocks": {
			"core/post-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--huge)",
					"fontWeight": "bold"
				},
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--30)",
						"bottom": "var(--wp--preset--spacing--50)"
					}
				}
			},
			"core/heading": {
				"typography": {
					"fontWeight": "bold"
				},
				"spacing": {
					"margin": {
						"top": "0.5em",
						"bottom": "0.25em"
					}
				},
				"elements": {
					"h1": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--huge)"
						}
					},
					"h2": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--x-large)"
						}
					},
					"h3": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--large)"
						}
					},
					"h4": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--medium)"
						}
					},
					"h5": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--normal)"
						}
					},
					"h6": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--small)"
						}
					}
				}
			},
			"core/paragraph": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)",
					"lineHeight": "1.5"
				},
				"spacing": {
					"padding": {
						"bottom": "1em"
					}
				}
			},
			"core/button": {
				"variations": {
					"outline": {
						"border": {
							"color": "currentColor",
							"style": "solid",
							"width": "1px"
						},
						"spacing": {
							"padding": {
								"top": "14px",
								"right": "29px",
								"bottom": "14px",
								"left": "29px"
							}
						}
					}
				}
			},
			"core/code": {
				"color": {
					"background": "var(--wp--preset--color--contrast)",
					"text": "var(--wp--preset--color--base)"
				},
				"css": "& code {overflow-wrap: normal; overflow-x: scroll; tab-size: 4; white-space: pre !important;}",
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--monospace)",
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-author-name": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "var(--wp--custom--line-height--medium)"
				}
			},
			"core/comment-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "var(--wp--custom--line-height--medium)"
				}
			},
			"core/comment-edit-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "var(--wp--custom--line-height--medium)"
				}
			},
			"core/comment-reply-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comments": {
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--x-large)"
					}
				}
			},
			"core/comments-title": {
				"spacing": {
					"margin": {
						"top": "var(--wp--custom--spacing--gap)",
						"bottom": "var(--wp--custom--spacing--gap)"
					}
				}
			},
			"core/image": {
				"css": "&.aligncenter {margin-left: auto; margin-right: auto;}"
			},
			"core/navigation": {
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-comments-form": {
				"css": "& input[type=submit] {width: auto;} & label {font-size: var(--wp--preset--font-size--x-small);}"
			},
			"core/post-template": {
				"css": "& .wp-block-post:not(:last-of-type) {margin-bottom: var(--wp--preset--spacing--medium);}"
			},
			"core/preformatted": {
				"color": {
					"background": "var(--wp--preset--color--contrast)",
					"text": "var(--wp--preset--color--base)"
				},
				"css": "{overflow-wrap: normal; overflow-x: scroll; tab-size: 4; white-space: pre !important;}",
				"spacing": {
					"padding": {
						"top": "25px",
						"right": "30px",
						"bottom": "25px",
						"left": "30px"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--monospace)",
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/pullquote": {
				"border": {
					"color": "var(--wp--preset--color--contrast)",
					"style": "solid",
					"width": "1px"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"lineHeight": "var(--wp--custom--line-height--medium)"
				}
			},
			"core/quote": {
				"border": {
					"color": "var(--wp--preset--color--contrast)",
					"style": "solid",
					"width": "1px"
				}
			},
			"core/separator": {
				"border": {
					"color": "currentColor",
					"radius": "0",
					"style": "solid",
					"width": "1px 0 0"
				},
				"css": ":not(.is-style-wide):not(.is-style-dots) {width: 100px;}"
			},
			"core/site-title": {
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"core/social-links": {
				"css": ".is-style-outline .wp-block-social-link {background-color: transparent;border: 1px solid currentColor;}"
			},
			"core/spacer": {
				"spacing": {
					"margin": {
						"top": "0 !important"
					}
				}
			},
			"core/table": {
				"css": "th {border-color: var(--wp--preset--color--contrast) !important;} & td {border-color: var(--wp--preset--color--contrast) !important;}",
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/template-part": {
				"spacing": {
					"margin": {
						"top": "0px !important"
					}
				}
			}
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "content",
			"title": "Content",
			"area": "uncategorized"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		}
	],
	"customTemplates": [
		{
			"name": "blank",
			"title": "Blank Tempate"
		}
	],
	"version": 2,
	"$schema": "https://schemas.wp.org/trunk/theme.json"
}
