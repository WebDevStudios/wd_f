{
	"version": 2,
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"settings": {
		"appearanceTools": true,
		"spacing": {
			"blockGap": true,
			"margin": true,
			"padding": true
		},
		"shadow": {
			"defaultPresets": false,
			"presets": [
				{
					"name": "Primary 200",
					"slug": "primary-200",
					"shadow": "0 4px 8px 0 rgba(59, 40, 204, 0.1), 0 0 1px 0 rgba(59, 40, 204, 0.05)"

				},
				{
					"name": "Primary 300",
					"slug": "primary-300",
					"shadow": "0 4px 8px 0 rgba(59, 40, 204, 0.1), 0 0 1px 0 rgba(59, 40, 204, 0.05)"
				},
				{
					"name": "Primary 400",
					"slug": "primary-400",
					"shadow": "0 8px 16px 0 rgba(59, 40, 204, 0.1), 0 0 1px 0 rgba(59, 40, 204, 0.05)"
				},
				{
					"name": "Primary 500",
					"slug": "primary-500",
					"shadow": "0 16px 24px 0 rgba(59, 40, 204, 0.09), 0 0 1px 0 rgba(59, 40, 204, 0.05)"
				},
				{
					"name": "Primary 600",
					"slug": "primary-600",
					"shadow": "0 24px 40px 0 rgba(59, 40, 204, 0.08), 0 0 1px 0 rgba(59, 40, 204, 0.05)"
				}
			]
		},
		"typography": {
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "Inter",
					"name": "Body",
					"slug": "body",
					"fontFace": [
						{
							"fontFamily": "Inter",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "300 400 700 900",
							"src": [
								"file:./src/fonts/inter/Inter.var.woff2"
							]
						}
					]
				},
				{
					"fontFamily": "Oxygen",
					"name": "Headline",
					"slug": "headline",
					"fontFace": [
						{
							"fontFamily": "Oxygen",
							"fontStyle": "normal",
							"fontWeight": "300",
							"src": [
								"file:./src/fonts/oxygen/oxygen-v15-latin-300.woff2"
							]
						},
						{
							"fontFamily": "Oxygen",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./src/fonts/oxygen/oxygen-v15-latin-regular.woff2"
							]
						},
						{
							"fontFamily": "Oxygen",
							"fontStyle": "normal",
							"fontWeight": "900",
							"src": [
								"file:./src/fonts/oxygen/oxygen-v15-latin-700.woff2"
							]
						}
					]
				}
			],
			"fontSizes": [
				{
					"fluid": {
						"min": "0.75rem",
						"max": "0.875rem"
					},
					"size": "0.875",
					"slug": "small",
					"name": "Small"
				},
				{
					"fluid": {
						"min": "0.875rem",
						"max": "1rem"
					},
					"size": "1rem",
					"slug": "normal",
					"name": "Normal"
				},
				{
					"fluid": {
						"min": "1rem",
						"max": "1.125rem"
					},
					"size": "1.125rem",
					"slug": "medium",
					"name": "Medium"
				},
				{
					"fluid": {
						"min": "1.75rem",
						"max": "1.875rem"
					},
					"size": "1.75rem",
					"slug": "large",
					"name": "Large"
				},
				{
					"fluid": false,
					"size": "2.25rem",
					"slug": "x-large",
					"name": "X Large"
				},
				{
					"fluid": {
						"min": "4rem",
						"max": "7rem"
					},
					"size": "7rem",
					"slug": "xx-large",
					"name": "XX Large"
				}
			]
		},
		"color": {
			"background": true,
			"custom": true,
			"customDuotone": true,
			"customGradient": true,
			"defaultGradients": false,
			"defaultPalette": false,
			"link": true,
			"text": true,
			"duotone": [],
			"gradients": [],
			"palette": [
				{
					"color": "#425a5e",
					"slug": "primary-100",
					"name": "Primary 100"
				},
				{
					"color": "#2d3d40",
					"slug": "primary-200",
					"name": "Primary 200"
				},
				{
					"color": "#3f4040",
					"slug": "primary-300",
					"name": "Primary 300"
				},
				{
					"color": "#fff",
					"slug": "secondary-100",
					"name": "Secondary 100"
				},
				{
					"color": "#e3e4eb",
					"slug": "secondary-200",
					"name": "Secondary 200"
				},
				{
					"color": "#d3d3d3",
					"slug": "secondary-300",
					"name": "Secondary 300"
				},
				{
					"color": "#f48d60",
					"slug": "tertiary-100",
					"name": "Tertiary 100"
				},
				{
					"color": "#f47c48",
					"slug": "tertiary-200",
					"name": "Tertiary 200"
				},
				{
					"color": "#f3713c",
					"slug": "tertiary-300",
					"name": "Tertiary 300"
				},
				{
					"color": "#22C55E",
					"slug": "success",
					"name": "Success"
				},
				{
					"color": "#EAB308",
					"slug": "warning",
					"name": "Warning"
				},
				{
					"color": "#EF4444",
					"slug": "error",
					"name": "Error"
				}
			]
		},
		"layout": {
			"contentSize": "840px",
			"wideSize": "1100px"
		}
	},

	"styles": {
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--body)",
			"fontSize": "var(--wp--preset--font-size--medium)"
		},
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--50)"
		},
		"color": {
			"background": "var(--wp--preset--color--white)",
			"text": "var(--wp--preset--color--black)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--headline)",
					"fontSize": "var(--wp--preset--font-size--xx-large)"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--headline)",
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--headline)",
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)"
				}
			},
			"button": {
				"border": {
					"radius": "10px",
					"style": "solid",
					"width": "0"
				},
				"color": {
					"background": "var(--wp--preset--color--tertiary-300)",
					"text": "var(--wp--preset--color--white)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)",
					"lineHeight": "1.435"
				},
				"spacing": {
					"padding": {
						"top": "8px",
						"right": "16px",
						"bottom": "8px",
						"left": "16px"
					}
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--tertiary-200)"
					}
				},
				":focus": {
					"color": {
						"background": "var(--wp--preset--color--tertiary-200)"
					}
				},
				":active": {
					"color": {
						"background": "var(--wp--preset--color--tertiary-100)"
					}
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--tertiary-300)"
				},
				":active": {
					"color": {
						"text": "var(--wp--preset--color--tertiary-200)"
					}
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--tertiary-200)"
					}
				},
				":focus": {
					"color": {
						"text": "var(--wp--preset--color--tertiary-200)"
					}
				}
			}
		},
		"blocks": {
			"core/post-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)",
					"fontWeight": "bold"
				},
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--30)",
						"bottom": "var(--wp--preset--spacing--50)"
					}
				}
			},
			"core/heading": {
				"typography": {
					"fontWeight": "bold"
				},
				"spacing": {
					"margin": {
						"top": "0.5em",
						"bottom": "0.25em"
					}
				},
				"elements": {
					"h1": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--xx-large)"
						}
					},
					"h2": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--x-large)"
						}
					},
					"h3": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--large)"
						}
					},
					"h4": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--medium)"
						}
					},
					"h5": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--normal)"
						}
					},
					"h6": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--small)"
						}
					}
				}
			},
			"core/paragraph": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)",
					"lineHeight": "1.5"
				},
				"spacing": {
					"padding": {
						"bottom": "1em"
					}
				}
			},
			"core/button": {
				"variations": {
					"outline": {
						"border": {
							"color": "currentColor",
							"style": "solid",
							"width": "1px"
						},
						"spacing": {
							"padding": {
								"top": "14px",
								"right": "29px",
								"bottom": "14px",
								"left": "29px"
							}
						}
					}
				}
			},
			"core/code": {
				"color": {
					"background": "var(--wp--preset--color--contrast)",
					"text": "var(--wp--preset--color--base)"
				},
				"css": "& code {overflow-wrap: normal; overflow-x: scroll; tab-size: 4; white-space: pre !important;}",
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--monospace)",
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-author-name": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "var(--wp--custom--line-height--medium)"
				}
			},
			"core/comment-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "var(--wp--custom--line-height--medium)"
				}
			},
			"core/comment-edit-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "var(--wp--custom--line-height--medium)"
				}
			},
			"core/comment-reply-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comments": {
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--x-large)"
					}
				}
			},
			"core/comments-title": {
				"spacing": {
					"margin": {
						"top": "var(--wp--custom--spacing--gap)",
						"bottom": "var(--wp--custom--spacing--gap)"
					}
				}
			},
			"core/image": {
				"css": "&.aligncenter {margin-left: auto; margin-right: auto;}"
			},
			"core/navigation": {
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-comments-form": {
				"css": "& input[type=submit] {width: auto;} & label {font-size: var(--wp--preset--font-size--x-small);}"
			},
			"core/post-template": {
				"css": "& .wp-block-post:not(:last-of-type) {margin-bottom: var(--wp--preset--spacing--medium);}"
			},
			"core/preformatted": {
				"color": {
					"background": "var(--wp--preset--color--contrast)",
					"text": "var(--wp--preset--color--base)"
				},
				"css": "{overflow-wrap: normal; overflow-x: scroll; tab-size: 4; white-space: pre !important;}",
				"spacing": {
					"padding": {
						"top": "25px",
						"right": "30px",
						"bottom": "25px",
						"left": "30px"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--monospace)",
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/pullquote": {
				"border": {
					"color": "var(--wp--preset--color--contrast)",
					"style": "solid",
					"width": "1px"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"lineHeight": "var(--wp--custom--line-height--medium)"
				}
			},
			"core/quote": {
				"border": {
					"color": "var(--wp--preset--color--contrast)",
					"style": "solid",
					"width": "1px"
				}
			},
			"core/separator": {
				"border": {
					"color": "currentColor",
					"radius": "0",
					"style": "solid",
					"width": "1px 0 0"
				},
				"css": ":not(.is-style-wide):not(.is-style-dots) {width: 100px;}"
			},
			"core/site-title": {
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"core/social-links": {
				"css": ".is-style-outline .wp-block-social-link {background-color: transparent;border: 1px solid currentColor;}"
			},
			"core/spacer": {
				"spacing": {
					"margin": {
						"top": "0 !important"
					}
				}
			},
			"core/table": {
				"css": "th {border-color: var(--wp--preset--color--contrast) !important;} & td {border-color: var(--wp--preset--color--contrast) !important;}",
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/template-part": {
				"spacing": {
					"margin": {
						"top": "0px !important"
					}
				}
			}
		}
	},

	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "content",
			"title": "Content",
			"area": "uncategorized"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		}
	],
	"customTemplates": [
		{
			"name": "blank",
			"title": "Blank"
		},
		{
			"name": "no-title",
			"title": "No Title"
		}
	]
}
